/* pages/records/rankList/index.wxss */
.cell {
    background: #f1f1f1;
    border-radius: 5px;
    display: flex;
    flex: row;
    margin: 5px 10px 10px 10px;
    height: 70px;
}

.leftContent {
    flex: 1;
    margin-top: 10px;
    margin-left: 10px;
}

.icon-name {
    display: flex;
    flex: row;
}

.icon {
    width: 30px;
    height: 30px;
    border-radius: 5px;
}

.name {
    color: #333;
    font-size: 17px;
    margin-left: 5px;
    margin-top: 3px;
}

.time {
    color: #999;
    font-size: 15px;
    margin-top: 5px;
}

.rightContent {
    width: 100px;
    margin-top: 10px;
    margin-right: 10px;
}

.score {
    text-align: center;
    color: #f00;
}

.prize {
    text-align: center;
    color: #999;
    font-size: 14px;
}